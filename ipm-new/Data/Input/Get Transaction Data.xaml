    <ui:MultipleAssign DisplayName="Multiple Assign" sap:VirtualizedContainerService.HintSize="438,101" sap2010:WorkflowViewState.IdRef="MultipleAssign_1">
      <ui:MultipleAssign.AssignOperations>
        <scg:List x:TypeArguments="ui:AssignOperation" Capacity="4">
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_1">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:String">[querySql]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">SELECT  cli.ID_CLIENTE,
        CNPJ,
        CODIGO,
        EMPRESA,

        EMITE_NFS,
        DECLARA_ENTRADA,
        LANCA_NOTA_ENTRADA,
        LOGIN,
        PASSWORD,
        CODIGO_LOGIN,
        CADASTRO_ECONOMICO,
		
        (CASE WHEN (PERIODO_DOWNLOAD_SAIDA &lt;&gt; '{PERIODO}' AND EMITE_NFS) THEN 'True' ELSE 'False' END) AS FAZ_DOWNLOAD_SAIDA,
        (CASE WHEN (PERIODO_DOWNLOAD_ENTRADA &lt;&gt; '{PERIODO}' AND DECLARA_ENTRADA) THEN 'True' ELSE 'False' END) AS FAZ_DOWNLOAD_ENTRADA,
        (CASE WHEN (PERIODO_DECLARACAO_SAIDA &lt;&gt; '{PERIODO}' AND EMITE_NFS) THEN 'True' ELSE 'False' END) AS FAZ_DECLARACAO_SAIDA,
        (CASE WHEN (PERIODO_DECLARACAO_ENTRADA &lt;&gt; '{PERIODO}' AND DECLARA_ENTRADA) THEN 'True' ELSE 'False' END) AS FAZ_DECLARACAO_ENTRADA,
        (CASE WHEN (PERIODO_SIMPLES_NACIONAL &lt;&gt; '{PERIODO}' AND (REGIME_TRIBUTARIO = 'A' OR REGIME_TRIBUTARIO = 'B') ) THEN 'True' ELSE 'False' END) AS FAZ_SIMPLES,
        
        
        PERIODO_DOWNLOAD_SAIDA,
        PERIODO_DOWNLOAD_ENTRADA,
        PERIODO_DECLARACAO_SAIDA,
        PERIODO_DECLARACAO_ENTRADA,
        PERIODO_SIMPLES_NACIONAL
        
		
FROM ipm faz
INNER JOIN _perfil per on per.ID_PERFIL = faz.ID_PERFIL
INNER JOIN ipm_cliente ipm on ipm.ID_CLIENTE = faz.ID_CLIENTE
INNER JOIN cliente cli on cli.ID_CLIENTE = ipm.ID_CLIENTE

WHERE  per.ID_PERFIL = 1 AND cli.SITUACAO = True AND per.SITUACAO = True</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
        </scg:List>
      </ui:MultipleAssign.AssignOperations>
    </ui:MultipleAssign>
