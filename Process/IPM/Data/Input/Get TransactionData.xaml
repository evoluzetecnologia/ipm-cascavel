    <ui:MultipleAssign DisplayName="Dados" sap:VirtualizedContainerService.HintSize="520,100" sap2010:WorkflowViewState.IdRef="MultipleAssign_2">
      <ui:MultipleAssign.AssignOperations>
        <scg:List x:TypeArguments="ui:AssignOperation" Capacity="4">
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_3">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:String">[QuerySQL]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">SELECT 	
	(CASE WHEN (SIM.ID_CLIENTE IS NOT NULL AND IPM.PERIODO_SIMPLES_NACIONAL &lt;&gt; '#PERIODO#') THEN TRUE ELSE FALSE END) AS FAZ_SIMPLES,
	(CASE WHEN (IPM.EMITE_NFS IS TRUE AND IPM.PERIODO_XML_SAIDA &lt;&gt; '#PERIODO#') THEN TRUE ELSE FALSE END) AS FAZ_XML_SAIDA,
    (CASE WHEN (IPM.DECLARACAO_TOMADOS IS TRUE AND IPM.PERIODO_XML_ENTRADA &lt;&gt; '#PERIODO#') THEN TRUE ELSE FALSE END) AS FAZ_XML_ENTRADA,
    (CASE WHEN (IPM.EMITE_NFS IS TRUE AND IPM.PERIODO_DECLARACAO_PRESTADOS &lt;&gt; '#PERIODO#') THEN TRUE ELSE FALSE END) AS FAZ_DECLARACAO_PRESTADOS,
    (CASE WHEN (IPM.DECLARACAO_TOMADOS IS TRUE AND IPM.PERIODO_DECLARACAO_TOMADOS &lt;&gt; '#PERIODO#') THEN TRUE ELSE FALSE END) AS FAZ_DECLARACAO_TOMADOS,
	IPM.ID_CLIENTE,
	CLI.CNPJ,
	CLI.EMPRESA,
	(CASE WHEN (IPM.IPM_LOGIN &lt;&gt; '' AND IPM.IPM_PASSWORD &lt;&gt; '') THEN TRUE ELSE FALSE END) AS ACESSO_CLIENTE,
	IPM.IPM_ID,
	IPM.IPM_LOGIN,
	IPM.IPM_PASSWORD,
	IPM.CADASTRO_ECONOMICO,
	IPM.IPM_CODIGO_LOGIN,

	(CASE WHEN (SIM.ID_CLIENTE IS NULL) THEN False ELSE True END) AS SIMPLES,
	(CASE WHEN (SIM.ID_CLIENTE IS NOT NULL) THEN (SIM.FATOR_R) ELSE True END) AS FATOR_R,
	(CASE WHEN (IMP.NFC_SAIDA OR IMP.NFE_SAIDA OR IMP.CTE_SAIDA) THEN TRUE ELSE FALSE END) AS OUTRAS_ATIVIDADES,

	IPM.LANCA_NOTA_TOMADOS,
	IPM.EMITE_NFS,
	IPM.DECLARACAO_TOMADOS,
	IPM.PERIODO_XML_SAIDA,
	IPM.PERIODO_XML_ENTRADA,
	IPM.PERIODO_DECLARACAO_PRESTADOS,
	IPM.PERIODO_DECLARACAO_TOMADOS,
    IPM.PERIODO_SIMPLES_NACIONAL,
	
	IPM.DIA_XML_SAIDA,
	IPM.DIA_XML_ENTRADA,
	IPM.DIA_DECLARACAO_PRESTADOS,
	IPM.DIA_DECLARACAO_TOMADOS,
	IPM.DIA_SIMPLES_NACIONAL
		
FROM ipm_cliente IPM
INNER JOIN cliente CLI ON CLI.ID_CLIENTE = IPM.ID_CLIENTE
LEFT JOIN cliente_simples_nacional SIM  ON SIM.ID_CLIENTE = CLI.ID_CLIENTE
LEFT JOIN dominio_fiscal_importacao IMP ON IMP.ID_CLIENTE = CLI.ID_CLIENTE

WHERE CLI.SITUACAO = 1

ORDER BY CLI.EMPRESA</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
        </scg:List>
