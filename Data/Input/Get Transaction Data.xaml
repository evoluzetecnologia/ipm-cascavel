            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">["SELECT  (CASE WHEN (
            (PERIODO_DOWNLOAD_SAIDA &lt;&gt; '{PERIODO}' AND DOWNLOAD_SAIDA &lt;&gt; 'Nenhum') OR 
            (PERIODO_DOWNLOAD_ENTRADA &lt;&gt; '{PERIODO}' AND DOWNLOAD_ENTRADA  &lt;&gt; 'Nenhum') OR
            (PERIODO_DECLARACAO_SAIDA &lt;&gt; '{PERIODO}' AND PROTOCOLAR_SAIDA  &lt;&gt; 'Nenhum') OR
            (PERIODO_DECLARACAO_ENTRADA &lt;&gt; '{PERIODO}' AND PROTOCOLAR_ENTRADA  &lt;&gt; 'Nenhum') OR
            (PERIODO_SIMPLES_NACIONAL &lt;&gt; '{PERIODO}' AND (REGIME_TRIBUTARIO = 'Microempresa' OR REGIME_TRIBUTARIO = 'Empresa de Pequeno Porte') AND DECLARAR_SIMPLES_NACIONAL)
         ) THEN 'True' ELSE 'False' END) AS EXECUTAR,
         
        (CASE WHEN (PERIODO_DOWNLOAD_SAIDA &lt;&gt; '{PERIODO}' AND DOWNLOAD_SAIDA &lt;&gt; 'Nenhum') THEN 'True' ELSE 'False' END) AS FAZ_DOWNLOAD_SAIDA,
        (CASE WHEN (PERIODO_DOWNLOAD_ENTRADA &lt;&gt; '{PERIODO}' AND DOWNLOAD_ENTRADA  &lt;&gt; 'Nenhum') THEN 'True' ELSE 'False' END) AS FAZ_DOWNLOAD_ENTRADA,
        (CASE WHEN (PERIODO_DECLARACAO_SAIDA &lt;&gt; '{PERIODO}' AND PROTOCOLAR_SAIDA  &lt;&gt; 'Nenhum') THEN 'True' ELSE 'False' END) AS FAZ_DECLARACAO_SAIDA,
        (CASE WHEN (PERIODO_DECLARACAO_ENTRADA &lt;&gt; '{PERIODO}' AND PROTOCOLAR_ENTRADA &lt;&gt; 'Nenhum') THEN 'True' ELSE 'False' END) AS FAZ_DECLARACAO_ENTRADA,
        (CASE WHEN (PERIODO_SIMPLES_NACIONAL &lt;&gt; '{PERIODO}' AND (REGIME_TRIBUTARIO = 'Microempresa' OR REGIME_TRIBUTARIO = 'Empresa de Pequeno Porte') AND DECLARAR_SIMPLES_NACIONAL) THEN 'True' ELSE 'False' END) AS FAZ_SIMPLES_NACIONAL, 
        
        cli.ID_CLIENTE,
        CNPJ,
        CODIGO,
        EMPRESA,
        LOGIN,
        PASSWORD,
        CODIGO_LOGIN,
        CODIGO_TOMADOR,
        CADASTRO_ECONOMICO,
        DOWNLOAD_SAIDA,
        DOWNLOAD_ENTRADA,
        PROTOCOLAR_SAIDA,
        PROTOCOLAR_ENTRADA,
        DECLARAR_SIMPLES_NACIONAL,
        LANCAR_NOTA_ENTRADA,
        PERIODO_DOWNLOAD_SAIDA,
        PERIODO_DOWNLOAD_ENTRADA,
        PERIODO_DECLARACAO_SAIDA,
        PERIODO_DECLARACAO_ENTRADA,
        PERIODO_SIMPLES_NACIONAL,
        
       'XML Entradas: ' || 
       (CASE WHEN (PERIODO_DOWNLOAD_SAIDA &lt;&gt; '{PERIODO}' AND DOWNLOAD_SAIDA &lt;&gt; 'Nenhum') THEN 'Sim' ELSE 'Não' END) || 
       ' | XML Saidas: ' ||
       (CASE WHEN (PERIODO_DOWNLOAD_ENTRADA &lt;&gt; '{PERIODO}' AND DOWNLOAD_ENTRADA  &lt;&gt; 'Nenhum') THEN 'Sim' ELSE 'Não' END) ||
       ' | Protocolar Saidas: ' ||
       (CASE WHEN (PERIODO_DECLARACAO_SAIDA &lt;&gt; '{PERIODO}' AND PROTOCOLAR_SAIDA  &lt;&gt; 'Nenhum') THEN 'Sim' ELSE 'Não' END) ||
       ' | Protocolar Entradas: ' ||
       (CASE WHEN (PERIODO_DECLARACAO_ENTRADA &lt;&gt; '{PERIODO}' AND PROTOCOLAR_ENTRADA  &lt;&gt; 'Nenhum') THEN 'Sim' ELSE 'Não' END) ||
       ' | Simples Nacional: ' ||
       (CASE WHEN (PERIODO_SIMPLES_NACIONAL &lt;&gt; '{PERIODO}' AND (REGIME_TRIBUTARIO = 'Microempresa' OR REGIME_TRIBUTARIO = 'Empresa de Pequeno Porte') AND DECLARAR_SIMPLES_NACIONAL) THEN 'Sim' ELSE 'Não' END) AS OBSERVACAO
        
FROM ipm
INNER JOIN _perfil per on per.ID_PERFIL = ipm.ID_PERFIL
INNER JOIN cliente cli on cli.ID_CLIENTE = ipm.ID_CLIENTE

WHERE  (per.ID_PERFIL = {Perfil} AND cli.SITUACAO = True AND per.SITUACAO = True) {QueryTransactionData}"]</InArgument>
