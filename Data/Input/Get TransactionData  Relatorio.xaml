            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">SELECT 	
    IPM.ID_CLIENTE,
    CLI.CNPJ,
    CLI.EMPRESA,
    (CASE WHEN (IPM.EMITE_NFS IS TRUE ) THEN '-' ELSE 'XXX' END) AS DOWNLOAD_XML_SAIDA,
    (CASE WHEN (IPM.DECLARACAO_TOMADOS IS TRUE) THEN '-' ELSE 'XXX' END) AS DOWNLOAD_XML_ENTRADA,
    (CASE WHEN (IPM.EMITE_NFS IS TRUE ) THEN '-' ELSE 'XXX' END) AS DECLARACAO_PRESTADOS,
    (CASE WHEN (IPM.DECLARACAO_TOMADOS IS TRUE ) THEN '-' ELSE 'XXX' END) AS DECLARACAO_TOMADOS,
    (CASE WHEN (SIM.ID_CLIENTE IS NOT NULL) THEN '-' ELSE 'XXX' END) AS SIMPLES_NACIONAL
		
FROM ipm_cliente IPM
INNER JOIN cliente CLI ON CLI.ID_CLIENTE = IPM.ID_CLIENTE
LEFT JOIN cliente_simples_nacional SIM  ON SIM.ID_CLIENTE = CLI.ID_CLIENTE
LEFT JOIN dominio_fiscal_importacao IMP ON IMP.ID_CLIENTE = CLI.ID_CLIENTE

WHERE CLI.SITUACAO = 1

ORDER BY CLI.EMPRESA</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
        </scg:List>
      </ui:MultipleAssign.AssignOperations>
